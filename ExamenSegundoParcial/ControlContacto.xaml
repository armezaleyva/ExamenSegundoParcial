<UserControl x:Class="ExamenSegundoParcial.ControlContacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Margin="8,8,8,0">Correo:</TextBlock>
        <TextBox Name="txtCorreo" Margin="8,8,8,0"></TextBox>
        <TextBlock Margin="8,8,8,0">Teléfono:</TextBlock>
        <TextBox Name="txtTelefono" Margin="8,8,8,0"></TextBox>
        <TextBlock Margin="8,8,8,0">Celular:</TextBlock>
        <TextBox Name="txtCelular" Margin="8,8,8,0"></TextBox>
    </StackPanel>
</UserControl>
